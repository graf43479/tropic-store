  <appSettings>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <add key="Email.WritAsFile" value="true"/>
    <add key="Glimpse:DisableAsyncSupport" value="true"/>
    <add key="enableSimpleMembership" value="false"/>
    <add key="html-minifier: Aggressive" value="true"/>
    <add key="html-minifier: Comments" value="true"/>
  </appSettings>
  <system.web>
    <!--Отключение сессии-->
    <!--<pages enableSessionState="false" />-->
    <globalization culture="ru-RU" uiCulture="ru-RU" requestEncoding="utf-8" responseEncoding="utf-8" responseHeaderEncoding="utf-8"/>
    <membership defaultProvider="CustomMembershipProvider">
      <providers>
        <clear/>
        <add name="CustomMembershipProvider" type="System.Web.Security.SqlMembershipProvider" connectionStringName="EFDbContext" maxInvalidPasswordAttempts="5" minRequiredPasswordLength="7" minRequiredNonalphanumericCharacters="0" passwordAttemptWindow="10"/>
        <!--ограничения-->
      </providers>
    </membership>
    <roleManager defaultProvider="CustomRoleProvider" enabled="true" cacheRolesInCookie="false">
      <providers>
        <clear/>
        <!--<add name="MyPrimaryRoleProvider" type="System.Web.Security.SqlRoleProvider" connectionStringName="EFDbContext"/>-->
        <add name="CustomRoleProvider" type="Domain.PrimaryRoleProvider" connectionStringName="EFDbContext" applicationName="/"/>
        <!--<add name="PrimaryRoleProvider" type="Domain.PrimaryRoleProvider" connectionStringName="EFDbContext" />-->
      </providers>
    </roleManager>
    <httpRuntime enableVersionHeader="true"/>
    <compilation debug="true" targetFramework="4.5">
      <assemblies>
        <add assembly="System.Web.Abstractions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
        <add assembly="System.Web.Helpers, Version=2.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
        <add assembly="System.Web.Routing, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
        <add assembly="System.Web.Mvc, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
        <add assembly="System.Web.WebPages, Version=2.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      </assemblies>
    </compilation>
    <!--<customErrors mode="On">
     <error statusCode="404" redirect="Error/NotFound" />
      </customErrors>-->
    <authentication mode="Forms">
      <forms loginUrl="~/Account/UserEnter" timeout="2880">
        <credentials passwordFormat="Clear">
          <!--  <user name="admin" password="secret" />
          <user name="admin2" password="secret2" />-->
        </credentials>
      </forms>
    </authentication>
    <pages>
      <namespaces>
        <!--<add namespace="MvcContrib.UI"/>
        <add namespace="MvcContrib.UI.Html"/>
        <add namespace="MvcContrib.UI.Grid"/>
        <add namespace="MvcContrib"/>-->
        <add namespace="System.Web.Helpers"/>
        <add namespace="System.Web.Mvc"/>
        <add namespace="System.Web.Mvc.Ajax"/>
        <add namespace="System.Web.Mvc.Html"/>
        <add namespace="System.Web.Routing"/>
        <add namespace="System.Web.WebPages"/>
      </namespaces>
    </pages>
    <!--<caching>
      <sqlCacheDependency enabled="true" pollTime="1000">
        <databases>
          <add name="SportStore" connectionStringName="EFDbContext"/>
        </databases>
      </sqlCacheDependency>
      <outputCacheSettings>
